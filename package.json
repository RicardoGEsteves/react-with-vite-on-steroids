    "husky": "9.1.6",
    "jsdom": "25.0.1",
    "lint-staged": "15.2.10",
    "postcss": "8.4.47",
    "prettier": "3.3.3",
    "tailwindcss": "3.4.14",
    "typescript": "5.6.3",
    "typescript-eslint": "8.11.0",
    "vite": "5.4.10",
    "vitest": "2.1.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "**/*": "prettier --write --ignore-unknown"
  }
